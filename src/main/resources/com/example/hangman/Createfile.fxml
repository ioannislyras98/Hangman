<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="109.0" prefWidth="546.0" style="-fx-background-color: #53c68c;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hangman.Controller">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Add a dictionary">
         <font>
            <Font name="Comic Sans MS" size="28.0" />
         </font>
      </Label>
      <TextField fx:id="AddDictionaryID" layoutX="286.0" layoutY="25.0" onAction="#buttonAdd" promptText="dictionary-ID" />
      <Button fx:id="buttonAdd" layoutX="466.0" layoutY="22.0" mnemonicParsing="false" onAction="#buttonAdd" prefHeight="32.0" prefWidth="61.0" text="Add">
         <font>
            <Font name="Comic Sans MS Bold Italic" size="15.0" />
         </font>
      </Button>
      <Label fx:id="status" layoutX="14.0" layoutY="65.0" prefHeight="24.0" prefWidth="515.0" text="  " textFill="#dd1e08">
         <font>
            <Font name="Comic Sans MS" size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
